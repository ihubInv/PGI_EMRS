{
  "name": "emrs-pgi-backend",
  "version": "1.0.0",
  "description": "Electronic Medical Record System for Psychiatry Department - PGIMER",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node database/supabaseMigrate.js migrate",
    "migrate:status": "node database/supabaseMigrate.js status",
    "seed": "node database/supabaseSeed.js seed",
    "seed:clear": "node database/supabaseSeed.js clear",
    "db:setup": "npm run migrate && npm run seed",
    "audit-fix": "npm audit fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "emr",
    "psychiatry",
    "medical",
    "records",
    "pgimer"
  ],
  "author": "PGIMER Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
